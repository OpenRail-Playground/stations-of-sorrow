# Environment variables for Stations of Sun

# Node environment - development, test, or production
NODE_ENV=development

# Server port
PORT=3000

# PostgreSQL database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=stations_of_sun
DB_USER=postgres
DB_PASSWORD=postgres

# Deutsche Bahn API credentials
DB_API_CLIENT_ID=your_client_id_here
DB_API_CLIENT_SECRET=your_client_secret_here

# CORS configuration
CORS_ORIGIN=*

# Socket.IO configuration
SOCKET_CORS_ORIGIN=*
